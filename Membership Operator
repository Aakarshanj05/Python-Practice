str = 'Aakarshan'
print('a' in str)
print('k' in str)
print('r' in str)
print('i' in str)

print('i' not in str)

print(round(11.5))
print(round(12.5))
print(round(654, -2))

name ='Krishna'
age = 30
height = 1.7

